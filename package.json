{
  "name": "vue3-ssr-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:ssr": "cross-env NODE_ENV=development nodemon --ignore dist/ -e ejs,js,ts,css,html,jpg,png,scss app.ts",
    "serve:refresh": "npm run bundle && npm run serve:ssr",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean": "rm -rf ./dist",
    "bundle": "npm run clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "vue-cli-service build",
    "bundle:server": "SSR=1 vue-cli-service build --dest dist/server",
    "bundle:app": "tsc --project tsconfig.app.json",
    "bundle:production": "npm run bundle:app && npm run bundle:client && npm run bundle:server"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.2.33",
    "compression": "^1.7.4",
    "core-js": "^3.8.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "@element-plus/icons-vue": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@stripe/stripe-js": "^1.27.0",
    "axios": "^0.26.1",
    "element-plus": "^2.1.8",
    "vue-i18n": "^9.1.0",
    "vue-i18n-extract": "^1.0.2",
    "vue-use-stripe": "^0.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "connect-livereload": "^0.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "file-loader": "^6.2.0",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "~4.5.5",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0",
    "@intlify/vue-i18n-loader": "^3.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "unplugin-auto-import": "^0.7.0",
    "unplugin-vue-components": "^0.19.0"
  }
}
